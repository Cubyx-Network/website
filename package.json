{
	"name": "website",
	"version": "0.2.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:ci": "prisma migrate deploy && vite build",
		"build:netlify": "NETLIFY=true vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"prisma": "prisma migrate dev",
		"prisma:deploy": "prisma migrate deploy",
		"postinstall": "prisma generate",
		"release": "release-it",
		"test": "exit 0"
	},
	"devDependencies": {
		"@release-it/conventional-changelog": "^7.0.0",
		"@sveltejs/adapter-netlify": "^2.0.8",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.27.5",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"autoprefixer": "^10.4.14",
		"daisyui": "^4.4.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.31",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"prisma": "^5.6.0",
		"release-it": "^16.1.5",
		"svelte": "^4.2.2",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.0.0",
		"vite": "4.5.1"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/kanit": "^5.0.17",
		"@fontsource/montserrat": "^5.0.15",
		"@fortawesome/fontawesome-svg-core": "^6.5.1",
		"@fortawesome/free-brands-svg-icons": "^6.5.1",
		"@fortawesome/free-regular-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@prisma/client": "^5.6.0",
		"@types/node": "^20.10.4",
		"buffer": "^6.0.3",
		"dotenv": "^16.3.1",
		"stream": "^0.0.2",
		"svelte-awesome": "^3.2.1",
		"tailwind-merge": "^2.0.0",
		"util": "^0.12.5"
	}
}
